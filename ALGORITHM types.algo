ALGORITHM types
VAR
   i : INTEGER := 26;
   f : FLOAT := 4.56;
   c : CHAR := 'c';
   s : STRING := 'str';
   b : BOOLEAN := TRUE;
BEGIN
   // this is a single line comment
   /* this is
   a multi line comment */
   f := i; // correct
   i := f; // wrong
   // we cannot mix types
   f := c; // wrong
   s := c; // correct
   b := i; // wrong
END